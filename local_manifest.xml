<manifest>
  <!-- Repositories -->

  <remote name="github" fetch="https://github.com" />
  <remote  name="lineage"
           fetch="https://github.com/LineageOS"
           review="https://review.lineageos.org/"
           revision="refs/heads/lineage-18.1" />

  <remove-project name="platform_bootable_recovery" />
  <project name="AOSP-whatever/platform_bootable_recovery" path="bootable/recovery" remote="github" revision="hos-rika" />
  <remove-project name="platform_build" />
  <project name="AOSP-whatever/platform_build_make" path="build/make" remote="github" revision="hos-rika">
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <remove-project name="platform_build_soong" />
